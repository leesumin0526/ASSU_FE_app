<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/assu_background"
    tools:context=".presentation.admin.mypage.AdminMypagePendingPartnershipFragment">

    <!-- 상단 여백 -->
    <View
        android:id="@+id/pending_mg1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.039"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <!-- 헤더 영역 -->
    <View
        android:id="@+id/pending_mg2"
        android:layout_width="0dp"
        android:layout_height="10dp"
        app:layout_constraintWidth_percent="0.0763"
        app:layout_constraintTop_toBottomOf="@id/pending_mg1"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/btn_pending_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_back"
        app:layout_constraintTop_toBottomOf="@id/pending_mg1"
        app:layout_constraintStart_toEndOf="@id/pending_mg2"/>

    <View
        android:id="@+id/pending_mg3"
        android:layout_width="0dp"
        android:layout_height="5dp"
        app:layout_constraintWidth_percent="0.033"
        app:layout_constraintStart_toEndOf="@id/btn_pending_back"
        app:layout_constraintTop_toBottomOf="@id/pending_mg1"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/btn_pending_back"
        app:layout_constraintBottom_toBottomOf="@id/btn_pending_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="계정 관리"
        android:fontFamily="@font/pretendard_semibold"
        android:textColor="@color/assu_font_main"
        android:textSize="20sp"/>

    <View
        android:id="@+id/pending_mg4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0213"
        app:layout_constraintTop_toBottomOf="@id/btn_pending_back"
        app:layout_constraintStart_toStartOf="@id/btn_pending_back"/>

    <View
        android:id="@+id/pending_mg5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0743"
        app:layout_constraintTop_toBottomOf="@id/btn_pending_back"
        app:layout_constraintStart_toStartOf="@id/btn_pending_back"/>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        app:layout_constraintTop_toBottomOf="@id/pending_mg5"
        app:layout_constraintStart_toEndOf="@id/pending_mg2">

        <View
            android:id="@+id/adm_account_mg7_1"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_block_opponent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_block_opponent_24dp"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg7_1"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/adm_account_mg7_2"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_block_opponent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="차단 관리"
            android:textColor="@color/assu_font_main"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg7_2"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/cl_adm_account_component1"
        app:layout_constraintStart_toEndOf="@id/pending_mg2">

        <View
            android:id="@+id/adm_account_mg8_1"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_adm_alarmSetting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_account_logout"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg8_1"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:id="@+id/adm_account_mg13_2"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_adm_alarmSetting"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="로그아웃"
            android:textColor="@color/assu_font_main"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg13_2"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/cl_adm_account_component2"
        app:layout_constraintStart_toEndOf="@id/pending_mg2">

        <View
            android:id="@+id/adm_account_mg10"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_adm_secession"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_secession"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg10"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:id="@+id/adm_account_mg11"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_adm_secession"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="회원탈퇴"
            android:textColor="@color/assu_font_main"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg11"/>

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>
</layout>
