<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".AdminPassiveRegisterFinishFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/iv_cross"
            android:layout_width="24dp"
            android:layout_height="24dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="9dp"
            android:src="@drawable/ic_cross"/>

        <View
            android:id="@+id/view_passive_register_mg1"
            android:layout_width="match_parent"
            android:layout_height="255dp"
            app:layout_constraintTop_toBottomOf="@id/iv_cross"/>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:orientation="vertical"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@id/view_passive_register_mg1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="등록 완료"
                android:textSize="30sp"
                android:textColor="@color/assu_font_main"
                android:fontFamily="@font/pretendard_semibold"/>

            <View
                android:id="@+id/view_passive_register_mg2"
                android:layout_width="match_parent"
                android:layout_height="10dp"/>


            <TextView
                android:id="@+id/tv_contract_passive_register_guide"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="계약서는 확인을 거쳐 등록될 예정입니다!"
                android:textColor="@color/assu_font_sub"
                android:textSize="20sp"
                android:fontFamily="@font/pretendard_regular"/>

            <View
                android:id="@+id/view_finish_review_mg3"
                android:layout_width="match_parent"
                android:layout_height="20dp"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_finish_review_see_my_review"
                android:layout_width="100dp"
                android:layout_height="34dp"
                android:layout_gravity="center">

                <ImageView
                    android:id="@+id/bg_image"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:src="@drawable/ic_home_basic_box"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />

                <TextView
                    android:id="@+id/btn_check_contract"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="계약서 확인하기"
                    android:textSize="13sp"
                    android:fontFamily="@font/pretendard_semibold"
                    android:textColor="@color/assu_font_sub"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>