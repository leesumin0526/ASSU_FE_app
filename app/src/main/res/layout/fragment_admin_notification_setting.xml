<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/assu_background"
    tools:context=".presentation.admin.mypage.AdminNotificationSettingFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/admin_noti_setting_bar_bg"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/assu_background"
            app:layout_constraintHeight_percent="0.0914"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <View
            android:id="@+id/admin_noti_setting_mg1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.0157"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/admin_noti_setting_mg2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@id/admin_noti_setting_bar_bg"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent="0.01816" />

        <ImageView
            android:id="@+id/btn_admin_noti_setting_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_location_search_left_arrow"
            app:layout_constraintStart_toEndOf="@id/admin_noti_setting_mg2"
            app:layout_constraintTop_toTopOf="@id/admin_noti_setting_bar_bg"
            app:layout_constraintBottom_toBottomOf="@id/admin_noti_setting_bar_bg"/>

        <TextView
            android:id="@+id/tv_admin_noti_setting_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_semibold"
            android:text="알림설정"
            android:textColor="@color/assu_font_main"
            android:textSize="21sp"
            app:layout_constraintBottom_toTopOf="@+id/admin_noti_setting_mg4"
            app:layout_constraintEnd_toStartOf="@id/admin_noti_setting_mg3"
            app:layout_constraintStart_toEndOf="@id/admin_noti_setting_mg2"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg1" />

        <View
            android:id="@+id/admin_noti_setting_mg3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@id/admin_noti_setting_bar_bg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent="0.03816" />

        <View
            android:id="@+id/admin_noti_setting_mg4"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@id/admin_noti_setting_bar_bg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.0157"
            app:layout_constraintStart_toStartOf="parent" />

        <View
            android:id="@+id/admin_noti_setting_mg5"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.0485"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg3"/>

        <View
            android:id="@+id/admin_noti_setting_mg6"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintWidth_percent="0.0865"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg5"/>

        <View
            android:id="@+id/admin_noti_setting_mg7"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintWidth_percent="0.0865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg5"/>


        <ImageView
            android:id="@+id/btn_admin_noti_setting_push_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_toggle_able"
            app:layout_constraintEnd_toStartOf="@id/admin_noti_setting_mg6"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg5"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PUSH 알림"
            android:fontFamily="@font/pretendard_medium"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@id/admin_noti_setting_mg7"
            app:layout_constraintTop_toTopOf="@id/btn_admin_noti_setting_push_toggle"
            app:layout_constraintBottom_toBottomOf="@id/btn_admin_noti_setting_push_toggle"/>


        <View
            android:id="@+id/admin_noti_setting_mg8"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.0442"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/btn_admin_noti_setting_push_toggle"/>

        <ImageView
            android:id="@+id/btn_admin_noti_setting_partnership_proposal_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_toggle_able"
            app:layout_constraintEnd_toStartOf="@id/admin_noti_setting_mg6"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg8"/>

        <TextView
            android:id="@+id/tv_admin_noti_proposal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="제휴건의 알림"
            android:fontFamily="@font/pretendard_medium"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@id/admin_noti_setting_mg7"
            app:layout_constraintTop_toTopOf="@id/btn_admin_noti_setting_partnership_proposal_toggle"
            app:layout_constraintBottom_toBottomOf="@id/btn_admin_noti_setting_partnership_proposal_toggle"/>

        <View
            android:id="@+id/admin_noti_setting_mg9"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.0257"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/btn_admin_noti_setting_partnership_proposal_toggle"/>

        <ImageView
            android:id="@+id/btn_admin_noti_setting_partnership_suggestion_toggle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_toggle_able"
            app:layout_constraintEnd_toStartOf="@id/admin_noti_setting_mg6"
            app:layout_constraintTop_toBottomOf="@id/admin_noti_setting_mg9"/>

        <TextView
            android:id="@+id/tv_admin_noti_suggestion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="제휴제안 알림"
            android:fontFamily="@font/pretendard_medium"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@id/admin_noti_setting_mg7"
            app:layout_constraintTop_toTopOf="@id/btn_admin_noti_setting_partnership_suggestion_toggle"
            app:layout_constraintBottom_toBottomOf="@id/btn_admin_noti_setting_partnership_suggestion_toggle"/>





    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
</layout>